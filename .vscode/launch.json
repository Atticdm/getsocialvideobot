{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Attach to Chrome (Profile 6 @9222)",
      "type": "pwa-chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${workspaceFolder}/src/*",
        "webpack:///*": "${workspaceFolder}/*",
        "webpack:///./*": "${workspaceFolder}/*",
        "vite:///*": "${workspaceFolder}/*",
        "ng:///*": "${workspaceFolder}/*"
      }
    }
    {
      "name": "Debug Railway (Chrome Profile 6)",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "https://zoological-rejoicing-production.up.railway.app/",
      "webRoot": "${workspaceFolder}",
      "userDataDir": "/Users/attic/Library/Application Support/Google/Chrome/Profile 6",
      "runtimeArgs": [
        "--profile-directory=Profile 6"
      ],
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${workspaceFolder}/src/*",
        "webpack:///*": "${workspaceFolder}/*",
        "webpack:///./*": "${workspaceFolder}/*",
        "vite:///*": "${workspaceFolder}/*",
        "ng:///*": "${workspaceFolder}/*"
      }
    }
    {
      "name": "Node: server.ts (ts-node)",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}/video-bot",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--inspect=9229",
        "--require",
        "ts-node/register/transpile-only",
        "src/web/server.ts"
      ],
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}